{
  "compilerOptions": {
    "lib": ["es2021"],
    "module": "ES2020",
    "outDir": "./dist/",
    "target": "es2021",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src/**/*.ts"]
}
  